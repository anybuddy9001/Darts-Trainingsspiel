<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" minHeight="-Infinity" minWidth="-Infinity" prefHeight="356.0"
            prefWidth="602.0"
            snapToPixel="false" xmlns="http://javafx.com/javafx/18"
            fx:controller="io.github.anybuddy9001.dartstrainingsspiel.LauncherController">
    <ImageView fitHeight="356.0" fitWidth="673.0" layoutX="-3.0" pickOnBounds="true" preserveRatio="true">
        <Image url="@../images/Dartgross.png"/>
    </ImageView>
    <MenuButton focusTraversable="false" layoutX="14.0" layoutY="318.0" mnemonicParsing="false" prefHeight="24.0"
                prefWidth="170.0" text="%launcherWindow.text.languageSelection">
        <items>
            <MenuItem mnemonicParsing="false" onAction="#setLanguageGerman" text="Deutsch"/>
            <MenuItem mnemonicParsing="false" onAction="#setLanguageEnglish" text="English"/>
        </items>
    </MenuButton>
    <Text fill="WHITE" layoutX="37.0" layoutY="68.0" strokeType="OUTSIDE" strokeWidth="0.0"
          text="%launcherWindow.text.title" textAlignment="CENTER" underline="true" wrappingWidth="527.40380859375">
        <font>
            <Font size="50.0"/>
        </font>
    </Text>
    <Text fx:id="activeLanguage" fill="WHITE" layoutX="20.0" layoutY="308.0" strokeType="OUTSIDE" strokeWidth="0.0"
          textAlignment="CENTER" wrappingWidth="158.7294921875">
        <font>
            <Font size="20.0"/>
        </font>
    </Text>
    <Button layoutX="301.0" layoutY="154.0" mnemonicParsing="false" onAction="#startChallengeMode" prefHeight="43.0"
            prefWidth="283.0" text="%launcherWindow.button.challengeMode"/>
    <Text fill="WHITE" layoutX="309.0" layoutY="222.0" strokeType="OUTSIDE" strokeWidth="0.0"
          text="%launcherWindow.text.duration" textAlignment="RIGHT">
        <font>
            <Font size="17.0"/>
        </font>
    </Text>
    <TextField fx:id="duration" alignment="CENTER" layoutX="395.0" layoutY="207.0" maxHeight="-Infinity"
               maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefColumnCount="1" prefHeight="20.0"
               prefWidth="45.0" promptText="10"/>
    <Text fill="WHITE" layoutX="446.0" layoutY="223.0" strokeType="OUTSIDE" strokeWidth="0.0" text="min"
          wrappingWidth="38.7294921875">
        <font>
            <Font size="15.0"/>
        </font>
    </Text>
    <Button layoutX="301.0" layoutY="234.0" mnemonicParsing="false" onAction="#startEndlessMode" prefHeight="43.0"
            prefWidth="283.0" text="%launcherWindow.button.endlessMode"/>
    <Button cancelButton="true" layoutX="518.0" layoutY="301.0" mnemonicParsing="false" onAction="#quit" text="Quit"/>
    <Text fill="WHITE" layoutX="450.0" layoutY="345.0" strokeType="OUTSIDE" strokeWidth="0.0" text="%version"
          textAlignment="RIGHT" wrappingWidth="135.099609375">
        <font>
            <Font size="10.0"/>
        </font>
    </Text>
</AnchorPane>
